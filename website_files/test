<!DOCTYPE html>
<html>

<script src="https://code.jquery.com/jquery-2.2.4.js"></script>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width">
<title>JS Bin</title>
<script src="https://npmcdn.com/parse/dist/parse.js"></script>


<head>

  <script>
  // ======================================================================= \/

  Parse.initialize("w5rew8rw4r8w7rw81fw8");
  Parse.serverURL = 'https://zj-tutorial-2018.herokuapp.com/parse';

  var GameScore = Parse.Object.extend("GameScore");
  var query = new Parse.Query(GameScore);
  query.get("bhgAsBvwg2", {
    success: function(gameScore) {
      // The object was retrieved successfully.


    var score = gameScore.get("playerName");

    document.write(score)


    },
    error: function(object, error) {
      // The object was not retrieved successfully.
      // error is a Parse.Error with an error code and message.
    }
  });

// ======================================================================= /\
  </script>



</head>

